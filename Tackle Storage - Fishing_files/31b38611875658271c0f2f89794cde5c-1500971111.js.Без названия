this.cancel=cancel;},_createScore:function(){var score=pjQuery_1_10_2(this.opt.targetScore);this.score=score.length?score:methods._buildScoreField.call(this);},_createStars:function(){for(var i=1;i<=this.opt.number;i++){var
attrs,icon=(this.opt.score&&this.opt.score>=i)?'starOn':'starOff',title=methods._getHint.call(this,i);icon=this.opt.path+this.opt[icon];if(this.opt.starType!=='img'){attrs={'data-alt':i,'class':icon};}else{attrs={src:icon,alt:i};}
