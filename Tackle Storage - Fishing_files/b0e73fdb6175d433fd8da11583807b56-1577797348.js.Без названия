function amshopby_slider_ui_update_values(prefix,values,slider){var parent=slider.parents('ol');var sliderFrom=parent.find('#'+prefix+'-from');var sliderTo=parent.find('#'+prefix+'-to');if(sliderFrom&&sliderTo){sliderFrom.html(values[0])
