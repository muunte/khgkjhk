return c;}
$this.controlMinicart=function(){if($this.conf.mincartIsClosed){$this.showMinicart();}else{$this.hideMinicart();}}
$this.updateMinicart=function(){var req=$this.getPreparedData({});pjQuery_1_10_2.ajax({url:$this.conf.updateMinicartUrl,data:req,type:'POST',dataType:'json',success:function(res){$this.processSuccessResponse(res,req);},error:function(){$this.processErrorResponse(req);}});}
$this.showMinicart=function(){$this.dispatchEvent('before_showMinicart');if(!$this.conf.micartTemplate){pjQuery_1_10_2('#pac-mini-cart').show();pjQuery_1_10_2('.minicart_link').addClass('pac-active').addClass('skip-active');if($this.getObjLength($this.items)>3||pjQuery_1_10_2('.pac-mini-products-list').height()>320){pjQuery_1_10_2('.pac-mini-products-list-hld').jScrollPane();}
