function amshopby_slider_ui_init(from,to,max,prefix,min,step,uiParamElement){var slider=$amQuery(uiParamElement).siblings('.amshopby-slider-ui');if(!slider||typeof slider=='undefined'){slider=$amQuery('#'+prefix+'-ui');}
